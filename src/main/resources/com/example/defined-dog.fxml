      <AnchorPane layoutX="132.0" layoutY="100.0" prefHeight="200.0" prefWidth="335.0" style="-fx-background-color: #FF8C00; -fx-border-width: 1; -fx-border-color: black;">
         <children>
            <Label fx:id="label_defined_dog" alignment="CENTER" layoutX="14.0" layoutY="22.0" prefHeight="155.0" prefWidth="307.0">
               <font>
                  <Font name="Times New Roman" size="15.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <Button fx:id="button_show_dog" layoutX="200.0" layoutY="318.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="200.0" style="-fx-background-color: #FF8C00;" text="Show Your Dog">
         <font>
            <Font name="Arial Rounded MT Bold" size="13.0" />
         </font>
      </Button>
      <Button fx:id="button_menu" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="63.0" style="-fx-background-color: #FF8C00;" text="Menu">
